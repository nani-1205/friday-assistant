# .env file
GOOGLE_API_KEY=YOUR_GOOGLE_AI_STUDIO_API_KEY

# MongoDB Configuration
MONGO_USER=your_mongo_user
MONGO_PASSWORD=your_mongo_password
MONGO_HOST=your_mongo_host_or_ip  # e.g., localhost or cluster0.xxxxx.mongodb.net
MONGO_PORT=27017 # Your MongoDB port (default is 27017)
MONGO_DB_NAME=friday_assistant_db
MONGO_COLLECTION_NAME=interactions
MONGO_AUTH_DB=admin # The database to authenticate against (often 'admin' or the db name itself)